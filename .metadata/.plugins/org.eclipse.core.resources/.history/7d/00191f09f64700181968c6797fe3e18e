import java.util.Scanner;


public class Exos {

	public static void main(String[] args) {
		
		int[] tab1, tab2;
		tab1 = new int[] {4,2,-1,6,7,3,9};		
		tab2 = tp116(tab1, 7);
		for (int i = 0; i < tab2.length; i++) {
			System.out.println(tab2[i] + " ");
		}

	}
	
	public static void tp51() {
		boolean ok = false;
		Scanner s; int in;
		while (!ok) {
			System.out.println("Entrer un chiffre entre 1 et 3");
			s = new Scanner(System.in);
			in = s.nextInt();
			if (in >= 1 && in <=3) {
				ok = true;
			}
			else {
				System.out.println("Chiffre incorrect");
			}
		}
		
		System.out.println("OK");
	}
	
	public static void tp52() {
		boolean ok = false;
		Scanner s; int in;
		while (!ok) {
			System.out.println("Entrer un chiffre entre 10 et 20");
			s = new Scanner(System.in);
			in = s.nextInt();
			if (in >= 10 && in <=20) {
				ok = true;
			}
			else if (in < 10){
				System.out.println("Plus grand !");
			}
			else {
				System.out.println("Plus petit !");
			}
		}
		
		System.out.println("OK");
	}
	
	public static void tp53() {
		Scanner s;
		int nb;
		s = new Scanner(System.in);
		System.out.println("Entrer un chiffre");
		nb = s.nextInt();
		for (int i = 1; i <= 10; i++) {
			System.out.println(nb + i);
		}
		s.close();
	}
	
	public static void tp55() {
		Scanner s;
		int in;
		s = new Scanner(System.in);
		System.out.println("Entrer un chiffre");
		in = s.nextInt();
		for (int i = 1; i <= 10; i++) {
			System.out.println(in + " X " +  i + " : " + in*i);
		}
		s.close();
	}
	
	public static void tp56(){
		Scanner s;
		int in;	int i = 1; int res = 0;
		s = new Scanner(System.in);
		System.out.println("Entrer un chiffre");
		in = s.nextInt();
		while (i <= in) {
			res += i;
			i++;
		}
		System.out.println(res);
		s.close();
	}
	
	public static void tp57() {
		Scanner s;
		int in;	int i = 1; int res = 1;
		s = new Scanner(System.in);
		System.out.println("Entrer un chiffre");
		in = s.nextInt();
		while (i <= in) {
			res *= i;
			i++;
		}
		System.out.println(res);
		s.close();
	}
	
	public static void tp58() {
		Scanner s;
		int max = 0; int in, res;
		for (int i = 0; i < 20; i++) {
			System.out.println("Entrer un chiffre");
			s = new Scanner(System.in);
			in = s.nextInt();
			if (in > max) {
				max = in;
			}
		}
		
		System.out.println("Maximum : " + max);
	}

	public static void tp61() {
		int tab[] = new int[]{0,0,0,0,0,0,0};
	}
	
	public static void tp63() {
		int tab[] = new int[9];
		Scanner s;
		for (int i = 0; i <9; i++) {
			System.out.println("Entrer une note");
			s = new Scanner(System.in);
			tab[i] = s.nextInt();
		}
		
		for (int i = 0; i<9; i++) {
			System.out.print(tab[i] + "; ");
		}
	}
	
	public static void tp68() {
		int tab[];
		Scanner s; int nb, in, pos, neg;
		System.out.println("Indiquer le nombre de valeurs qui seront saisies");
		s = new Scanner(System.in);
		nb = s.nextInt();
		tab = new int[nb];
		pos = neg = 0;
		for (int i = 0; i < nb; i++) {
			System.out.println("Entrer une valeur");
			s = new Scanner(System.in);
			in = s.nextInt();
			tab[i] = in;
			if (in >= 0) {
				pos ++;
			} else {
				neg ++;
			}			
		}
		s.close();
		
		System.out.println(pos + " nombres négatifs, " + neg + " nombres négatifs.");
	}
	
	public static void tp610() {
		int tab1[], tab2[], tab3[];
		tab1 = new int[] {1, 12, 14, 8, 7, 6, 9};
		tab2 = new int[] {3, 13, 4, 18, 17, 8, 5};
		tab3 = new int[tab1.length];
		for (int i = 0; i < tab1.length; i++) {
			tab3[i] = tab1[i] + tab2[i];
		}
		
		for (int i = 0; i< tab1.length; i++) {
			System.out.print(tab3[i] + "; ");
		}
	}

	public static int tp111(int in1, int in2, int in3, int in4, int in5) {
		int ret = 0;
		ret = in1 + in2 + in3 + in4 + in5;		
		return ret;
	}
	
	public static String tp114(String stin, String charin) {
		String ret = stin;
		if (stin.contains(charin)){
			ret = stin.replaceAll(charin, "");
		}	
		return ret;
	}

	public static int[] tp116(int tab[], int taille){
		
		int res[] = new int[taille];
		int posMini;
		// tab[min] est inférieur à tout le reste mais supérieur à res[j-1]
		
		for (int j = 0; j < taille; j++) {
			posMini = 0;
			for (int i = 0; i < taille; i ++) {
				if (tab[i] < tab[posMini]) {
					if (j!=0) {
						if (tab[i] >= res[j-1]) {
							posMini = i;
						}
					}
					posMini = i;
				}				
			}
			res[j] = tab[posMini];
		}
		
		return res;
	

		
	}
}


